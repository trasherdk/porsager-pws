{
  "name": "pws",
  "version": "7.0.0",
  "description": "A Persistent WebSocket wrapper",
  "type": "module",
  "module": "pws.js",
  "browser": "pws.umd.js",
  "main": "pws.umd.js",
  "exports": {
    "import": "./pws.js",
    "default": "./umd/pws.js"
  },
  "repository": "porsager/pws",
  "keywords": [
    "websocket",
    "reconnect",
    "ubre",
    "ping",
    "timeout"
  ],
  "scripts": {
    "test": "node test.js",
    "prepublishOnly": "npm run build",
    "build": "node -e 'fs.writeFileSync(\"umd/pws.js\",\";(function(g){\"+fs.readFileSync(\"pws.js\",\"utf8\").replace(\"export default pws\",`typeof exports===\"object\"&&typeof module\\!==\"undefined\"?module.exports=pws:((g?g:self).pws=pws);`) + \"})(this);\")'"
  },
  "author": "Rasmus Porsager <rasmus@porsager.com>",
  "license": "WTFPL",
  "homepage": "https://github.com/porsager/pws#readme",
  "bugs": {
    "url": "https://github.com/porsager/pws/issues"
  },
  "devDependencies": {
    "fantestic": "0.0.3",
    "ws": "8.17.1"
  }
}
